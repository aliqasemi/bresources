{
    "name": "behamin/bresources",
    "description": "Behamin basic and standard api response format for laravel",
    "keywords": [
        "behamin",
        "request",
        "resource",
        "laravel",
        "package",
        "response",
        "json",
        "collection"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Omid Alizadeh",
            "email": "om_alizadeh@yahoo.com"
        },
        {
            "name": "Hossein Ebrahimzadeh",
            "email": "abi.hossein@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Behamin\\BResources\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Behamin\\BResources\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Behamin\\BResources\\BResourceServiceProvider"
            ]
        }
    },
    "require": {
      "php": "^7.3|^8.0",
      "laravel/framework": "~6.0|~7.0|~8.0",
      "ext-json": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}